    "extract-text-webpack-plugin": "^1.0.1",
    "glob": "^7.0.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "install": "^0.8.1",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "marked": "^0.3.6",
    "node-sass": "^3.8.0",
